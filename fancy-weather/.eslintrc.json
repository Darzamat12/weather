{
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "globals":{
        "document":true
    },
    "rules": {
        "semi": "error",
        "linebreak-style": 0
    },
    "env": {
        "browser": true,
        "es6": true
    }
  }